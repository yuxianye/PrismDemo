<UserControl x:Class="PrismDemo.HelloWorld.ModelA.Views.ViewA"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
             xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
             xmlns:local="clr-namespace:PrismDemo.HelloWorld.ModelA.Views"
             xmlns:prism="http://prismlibrary.com/"
             prism:ViewModelLocator.AutoWireViewModel="True"
             mc:Ignorable="d"
             d:DesignHeight="300"
             d:DesignWidth="300">
    <Grid>
        <StackPanel>
            <TextBlock Text="{Binding Title}"></TextBlock>
            <Button Content="按钮"></Button>
            <CheckBox Content="复选框"></CheckBox>
            <RadioButton Content="单选框"></RadioButton>

        </StackPanel>
    </Grid>
</UserControl>
